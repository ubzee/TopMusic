<body class="home">
    <!-- Fixed navbar -->
    <div class="navbar navbar-inverse navbar-fixed-top headroom">
        <div class="container">
            <div class="navbar-header">
                <!-- Button for smallest screens -->
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"><span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                <a class="navbar-brand" href="index.html"><img src="~/Content/images/logo.png" alt="Progressus HTML5 template"></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav pull-right">
                    <li class="active">@Html.ActionLink("Home", "Index", "Home", new { Area = "" }, null)</li>
                    <li>@Html.ActionLink("Classement", "Classement", "Home", new { Area = "" }, null)</li>
                    
                    @if (User.IsInRole("Administrator"))
                    {
                        <li>@Html.ActionLink("Roles", "Index", "Role", new { Area = "Admin" }, null)</li>
                        <li>@Html.ActionLink("Categories", "Index", "Category", new { Area = "Admin" }, null)</li>

                    }
                    @Html.Partial("_LoginPartial")
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </div>
